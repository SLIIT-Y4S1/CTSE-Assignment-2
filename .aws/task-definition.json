{
  "family": "ctse-task-def",
  "containerDefinitions": [
    {
      "name": "ctset-devops-container",
      "image": "992382538912.dkr.ecr.us-east-1.amazonaws.com/ctse-devops",
      "cpu": 0,
      "portMappings": [
        {
          "name": "ctset-devops-container-5000-tcp",
          "containerPort": 5000,
          "hostPort": 5000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "PORT",
          "value": "5000"
        },
        {
          "name": "NODE_ENV",
          "value": "production"
        }
      ],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "secrets": [
        {
          "name": "MONGO_URI",
          "valueFrom": "CTSE-MONGO_URI"
        },
        {
          "name": "DB_NAME",
          "valueFrom": "CTSE-DB_NAME"
        },
        {
          "name": "GRAFANA_LOKI_HOST",
          "valueFrom": "CTSE-GRAFANA_LOKI_HOST"
        },
        {
          "name": "GRAFANA_LOKI_USERID",
          "valueFrom": "CTSE-GRAFANA_LOKI_USERID"
        },
        {
          "name": "GRAFANA_LOKI_API_TOKEN",
          "valueFrom": "CTSE-GRAFANA_LOKI_API_TOKEN"
        }
      ],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/ctse-task-def",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "systemControls": []
    }
  ],
  "executionRoleArn": "arn:aws:iam::992382538912:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
